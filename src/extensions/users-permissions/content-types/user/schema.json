{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "customer_role": {
      "type": "string",
      "default": "user"
    },
    "user_avatar": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "user_gender": {
      "type": "string"
    },
    "date_of_birth": {
      "type": "date"
    },
    "user_cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "user_first_name": {
      "type": "string"
    },
    "user_last_name": {
      "type": "string"
    },
    "user_address": {
      "type": "string"
    },
    "user_LLC": {
      "type": "string"
    },
    "user_description": {
      "type": "string"
    },
    "user_vimeo": {
      "type": "string"
    },
    "user_instagram": {
      "type": "string"
    },
    "user_facebook": {
      "type": "string"
    },
    "user_twitter": {
      "type": "string"
    },
    "created_videos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::video.video",
      "mappedBy": "user_creator"
    },
    "user_playlist": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::playlist.playlist",
      "mappedBy": "playlists_user"
    },
    "store": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::store.store",
      "mappedBy": "users_permissions_user"
    },
    "user_stores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::store.store",
      "mappedBy": "store_author"
    },
    "user_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "comment_author"
    }
  }
}
