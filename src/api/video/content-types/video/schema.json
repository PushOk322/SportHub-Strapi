{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "Video"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "video_file": {
      "allowedTypes": [
        "videos"
      ],
      "type": "media",
      "multiple": true
    },
    "video_name": {
      "type": "string"
    },
    "video_length": {
      "type": "integer"
    },
    "video_preview": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "video_type": {
      "type": "string",
      "default": "mind"
    },
    "video_description": {
      "type": "string"
    },
    "video_shop_link": {
      "type": "string"
    },
    "video_likes": {
      "type": "integer"
    },
    "video_dislikes": {
      "type": "integer"
    },
    "video_views": {
      "type": "integer"
    },
    "user_creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "created_videos"
    },
    "video_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "comment_video"
    }
  }
}
